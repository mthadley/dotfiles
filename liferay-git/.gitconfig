[user]
	email = michael.hadley@liferay.com
	name = Michael Hadley
[core]
	editor = vim
	autocrlf = input
	pager = less -x4
	excludesfile = /Users/mike/.gitignore
[alias]
	oldest-ancestor = !zsh -c 'diff --old-line-format= --new-line-format= <(git rev-list --first-parent \"${1:-master}\") <(git rev-list --first-parent \"${2:-HEAD}\") | head -1' -
